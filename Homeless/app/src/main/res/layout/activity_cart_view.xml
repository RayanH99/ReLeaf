                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="sans-serif-light"
                android:text="Toothbrush"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/toothbrushCosts"
                app:layout_constraintStart_toEndOf="@+id/bananaClipart"
                app:layout_constraintTop_toBottomOf="@+id/bananaCost"
                app:layout_constraintVertical_bias="0.526" />

            <ImageView
                android:id="@+id/bananaClipart"
                android:layout_width="37dp"
                android:layout_height="42dp"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="25dp"
