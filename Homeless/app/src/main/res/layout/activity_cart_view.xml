                app:srcCompat="@drawable/banana_clipart" />

            <TextView
                android:id="@+id/bananaCost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:text="$3.47"
                android:textColor="#0000cd"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@+id/bananaClipart"
                app:layout_constraintTop_toBottomOf="@+id/bananaEntry" />


            <ImageView
                android:id="@+id/toothbrushClipart"
                android:layout_width="37dp"
                android:layout_height="42dp"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/goalClipart"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bananaCost"
