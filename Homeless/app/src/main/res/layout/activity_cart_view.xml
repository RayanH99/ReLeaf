            <ListView
                android:id="@+id/brushDivider"
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginBottom="8dp"
                android:background="#d3d3d3"
                app:layout_constraintBottom_toTopOf="@+id/goalClipart"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/toothbrushCosts" />

            <ListView
                android:id="@+id/FundDivider"
                android:layout_width="wrap_content"
